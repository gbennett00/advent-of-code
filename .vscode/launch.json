{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug AoC Day (tsx)",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "runtimeArgs": ["--inspect-brk", "./node_modules/tsx/dist/cli.mjs"],
        "args": ["src/run-day.ts", "5"],
        "cwd": "${workspaceFolder}",
        "skipFiles": ["<node_internals>/**"],
        "console": "integratedTerminal"
      }
    ]
  }
  